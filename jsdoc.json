{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.ts$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["node_modules/jsdoc-ts-path"],
  "opts": {
    "destination": "./docs",
    "recurse": true
  },
  "tags": {
    "allowUnknownTags": true
  },
  "typescript": {
    "moduleRoot": "src"
  }
}
