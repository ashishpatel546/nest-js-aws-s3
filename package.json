{
  "name": "@sologence/nest-js-aws-s3",
  "version": "1.0.3",
  "description": "NestJS AWS S3 Module that can easily upload, download, delete and list files from AWS S3 bucket.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "docs": "typedoc",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --out docs && node scripts/update-readme.js",
    "docs:update": "npm run docs:md && node scripts/update-readme.js",
    "docs:generate": "typedoc",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "nestjs",
    "aws",
    "s3",
    "storage",
    "upload",
    "file-storage",
    "cloud-storage"
  ],
  "author": "Ashish Kumar",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.511.0",
    "@aws-sdk/s3-request-presigner": "^3.511.0",
    "csv-stringify": "^6.4.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "typedoc": "^0.25.8",
    "typedoc-plugin-markdown": "^4.0.0-next.53",
    "typescript": "^5.3.3"
  },
  "homepage": "https://github.com/ashishpatel546/nest-js-aws-s3#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashishpatel546/nest-js-aws-s3.git"
  },
  "bugs": {
    "url": "https://github.com/ashishpatel546/nest-js-aws-s3/issues"
  }
}
