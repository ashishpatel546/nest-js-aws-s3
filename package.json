{
  "name": "@solegence/nest-js-aws-s3",
  "version": "1.0.0",
  "description": "NestJS AWS S3 Module that can easily upload, download, delete and list files from AWS S3 bucket.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "docs": "typedoc",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --out docs && node scripts/update-readme.js",
    "docs:update": "npm run docs:md && node scripts/update-readme.js",
    "docs:generate": "typedoc",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "nestjs",
    "aws",
    "s3",
    "storage",
    "upload",
    "file-storage",
    "cloud-storage"
  ],
  "author": "Ashish Kumar",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "csv-stringify": "^6.5.2"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.15.3"
  },
  "homepage": "https://github.com/ashishpatel546/nest-js-aws-s3#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashishpatel546/nest-js-aws-s3.git"
  },
  "bugs": {
    "url": "https://github.com/ashishpatel546/nest-js-aws-s3/issues"
  }
}
