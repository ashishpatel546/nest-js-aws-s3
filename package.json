  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.15.3"
  },
  "homepage": "https://github.com/yourusername/nest-js-aws-s3#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/nest-js-aws-s3.git"
